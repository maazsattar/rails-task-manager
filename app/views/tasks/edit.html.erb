<h1>Edit Task</h1>
<%= form_with model: @task do |f| %>
  <p><%= f.label :title %> <br>
  <%= f.text_field :title %></p>
  <p><%= f.label :details %><br>
  <%= f.text_field :details %></p>
  <p><%= f.label :completed  %> <br>
  <%= f.check_box :completed %></p>
  <%= f.submit%>
<% end %>
<p class = "btn btn-primary" style="margin-top: 15px"><%= link_to "Back", list_path,  class: "button" %></p>
